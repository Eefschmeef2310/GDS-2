[sub_resource type="GDScript" id="GDScript_kqd5g"]
resource_name = "testing_player"
script/source = "extends Kart
	#class_name
#Authored by Ethan. Please consult for any modifications or major feature requests.

#THIS CLASS IS PURELY FOR THE TEST TRACK, SO THAT I CAN BYPASS HAVING TO SET UP MULTIPLAYER INPUT.
#THIS CLASS ASSUMES YOU HAVE A CONTROLLER PLUGGED IN (INPUT DEVICE HARD CODED TO 0)

# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(_delta: float) -> void:
	if is_player and can_control:
		steer_axis = MultiplayerInput.get_axis(0, \"Right\", \"Left\")
		accelerating = MultiplayerInput.is_action_pressed(0, \"Accelerate\")
		braking = MultiplayerInput.is_action_pressed(0, \"brake\")
		drift_input = MultiplayerInput.is_action_pressed(0, \"drift\")
		drift_released = !drift_input
"

